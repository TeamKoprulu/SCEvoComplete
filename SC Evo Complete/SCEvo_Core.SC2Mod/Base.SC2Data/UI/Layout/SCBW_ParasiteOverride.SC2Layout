    <Frame type="CargoButton" name="CargoButtonTemplate" file="InfoPaneCargo">
        <Frame type="UnitStatusFrame" name="UnitStatusFrame" template="SCBW_ParasiteOverride/ParasitedUnitStatusTemplate"/>
